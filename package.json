{
  "name": "web-app",
  "version": "1.0.0",
  "description": "The front-end AngularJS app and server for WECO.",
  "main": "server.js",
  "scripts": {
    "beanstalk:dev": "eb deploy webapp-dev --profile weco-iam",
    "beanstalk:production": "eb deploy webapp-prod --profile weco-iam",
    "build:dev": "npm-run-all gulp:dev",
    "build:production": "npm-run-all gulp:production",
    "deploy:dev": "npm-run-all build:dev upload:dev",
    "deploy:production": "npm-run-all build:production upload:production",
    "gulp:dev": "gulp --env=development",
    "gulp:production": "gulp --env=production",
    "lint": "eslint .",
    "start": "gulp",
    "start-server": "node server.js",
    "test": "npm run lint",
    "upload:dev": "npm-run-all beanstalk:dev",
    "upload:production": "npm-run-all beanstalk:production"
  },
  "repository": {
    "type": "git",
    "url": "https://MSChristensen@bitbucket.org/wecollective/wecowebapp.git"
  },
  "author": "WE Collective",
  "license": "ISC",
  "homepage": "https://weco.io",
  "dependencies": {
    "angular": "^1.5.7",
    "angular-animate": "^1.5.7",
    "angular-bind-html-compile": "^1.4.1",
    "angular-cache": "^4.6.0",
    "angular-chart.js": "^1.1.1",
    "angular-marked": "^1.2.2",
    "angular-resource": "^1.5.7",
    "angular-sanitize": "^1.5.7",
    "angular-ui-router": "^0.4.3",
    "chalk": "1.1.3",
    "chart.js": "^2.5.0",
    "dotenv": "^4.0.0",
    "es6-promise-polyfill": "^1.2.0",
    "express": "^4.13.4",
    "helmet": "^2.1.1",
    "highlightjs": "^8.7.0",
    "marked": "^0.3.6",
    "ng-file-upload": "^12.0.4",
    "prerender-node": "^2.4.0",
    "raven-js": "^3.22.0",
    "socket.io": "^2.2.0",
    "viewport-units-buggyfill": "^0.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "gulp": "^4.0.0",
    "gulp-less": "^4.0.1",
    "gulp-nodemon": "^2.4.2",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "plugin-error": "^1.0.1",
    "webpack": "^2.6.1",
    "yargs": "^7.0.2"
  }
}
